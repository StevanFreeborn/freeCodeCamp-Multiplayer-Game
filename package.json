{
  "name": "freecodecamp-multiplayer-game",
  "version": "1.0.0",
  "description": "A multiplayer game built as part of the freeCodeCamp Information Security Certification",
  "main": "server.mjs",
  "scripts": {
    "start": "node server.mjs",
    "dev": "NODE_ENV=development nodemon server.mjs",
    "dev-windows": "set NODE_ENV=development&& nodemon server.mjs",
    "test": "PORT=3005 mocha --require @babel/register --recursive --exit --ui tdd tests/",
    "test-windows": "set PORT=3005&& mocha --require @babel/register --recursive --exit --ui tdd tests/"
  },
  "dependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.3",
    "jsdom": "^16.2.0",
    "mocha": "^10.1.0",
    "morgan": "^1.10.0",
    "nocache": "^3.0.4",
    "nodemon": "^2.0.2",
    "socket.io": "^4.5.3",
    "socket.io-client": "^4.5.3",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "license": "MIT"
}
